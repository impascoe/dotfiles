{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "closer": "p:os",
        "pink": "#F5BDE6",
        "lavender": "#B7BDF8",
        "blue":  "#8AADF4",
        "e_blue": "#299EE0",
        "teal": "#8BD5CA"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:e_blue",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "p:teal",
          "style": "plain",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Name }}{{ .Name }}{{ end }} {{ if .Version }}v{{.Version}}{{ end }}{{ end }} ",
          "type": "project"
        },
        {
          "foreground": "p:teal",
          "style": "plain",
          "properties": {
            "fetch_version": false,
            "display_mode": "files",
            "home_enabled": true
          },
          "template": "\ue627{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "foreground": "p:teal",
          "style": "plain",
          "properties": {
            "fetch_version": true,
            "display_mode": "files",
            "home_enabled": true
          },
          "template": "\uE7BA{{ if .Error }}{{ .Error }}{{ else }}{{ end }} ",
          "type": "react"
        },
        {
          "foreground": "p:teal",
          "style": "plain",
          "properties": {
            "fetch_version": true,
            "display_mode": "files",
            "home_enabled": true
          },
          "template": "\uE738{{ if .Error }}{{ .Error }}{{ else }} {{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "foreground": "p:teal",
          "style": "plain",
          "properties": {
            "fetch_virtual_env": true,
            "display_mode": "context",
            "home_enabled": true
          },
          "template": "\udb80\udf20 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} (.venv) {{ end }}{{ end }}",
          "type": "python"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105\uF105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
