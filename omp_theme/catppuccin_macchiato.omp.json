{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#8AADF4",
    "closer": "p:os",
    "e_blue": "#299EE0",
    "lavender": "#B7BDF8",
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "teal": "#8BD5CA"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "foreground": "p:blue",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": "{{ .Path }} ",
          "foreground": "p:pink",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "foreground": "p:e_blue",
          "type": "git",
          "style": "plain"
        },
        {
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Name }}{{ .Name }}{{ end }} {{ if .Version }}v{{.Version}}{{ end }}{{ end }} ",
          "foreground": "p:teal",
          "type": "project",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": false,
            "home_enabled": true
          },
          "template": "\ue627{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:teal",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": true
          },
          "template": "\ue7ba{{ if .Error }}{{ .Error }}{{ else }}{{ end }} ",
          "foreground": "p:teal",
          "type": "react",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true,
            "home_enabled": true
          },
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:teal",
          "type": "java",
          "style": "plain"
        },
        {
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "template": "\udb80\udf20 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} (.venv) {{ end }}{{ end }}",
          "foreground": "p:teal",
          "type": "python",
          "style": "plain"
        },
        {
          "template": "\n\uf105\uf105",
          "foreground": "p:closer",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
